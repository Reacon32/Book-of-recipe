<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="312dp"
        android:layout_height="44dp"
        android:layout_marginTop="64dp"
        android:ems="10"
        android:inputType="text"
        android:text="Введите название или ингридиенты"
        android:textColor="#6B000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="136dp"
        android:layout_height="58dp"
        android:text="Вкусная, острая, прожаренная до корочки пицца"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="367dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="136dp"
        android:layout_height="58dp"
        android:text="Пицца из 100 сыров"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="620dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="136dp"
        android:layout_height="58dp"
        android:text="Пицца из Рима"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="617dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="187dp"
        android:layout_height="158dp"
        app:srcCompat="@drawable/pizza"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="179dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Маргарита"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="267dp"
        tools:layout_editor_absoluteY="340dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Пицца 100 сыров"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="590dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Римская пицца"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="590dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="136dp"
        android:layout_height="58dp"
        android:text="Пицца с томатами и тройным сыром"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="247dp"
        tools:layout_editor_absoluteY="367dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="114dp"
        android:layout_height="30dp"
        android:lineSpacingExtra="24sp"
        android:text="Пеперони"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="337dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="124dp"
        android:layout_height="53dp"
        android:layout_marginBottom="564dp"
        android:text="Поиск"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="194dp"
        android:layout_height="158dp"
        app:srcCompat="@drawable/pizza1"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="179dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Назад"
        tools:layout_editor_absoluteX="1dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="184dp"
        android:layout_height="154dp"
        app:srcCompat="@drawable/pizza100cirov"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="436dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="183dp"
        android:layout_height="166dp"
        app:srcCompat="@drawable/rimskaipizza"
        tools:layout_editor_absoluteX="212dp"
        tools:layout_editor_absoluteY="424dp" />

</androidx.constraintlayout.widget.ConstraintLayout>